{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$components": ["src/components"],
      "$components/*": ["src/components/*"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"],
      "$public": ["src/public"],
      "$public/*": ["src/public/*"],
      "$styles": ["src/styles"],
      "$styles/*": ["src/styles/*"]
    }
  }
}
